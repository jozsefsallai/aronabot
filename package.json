{
  "name": "aronabot",
  "version": "1.0.0",
  "description": "Blue Archive themed Discord bot.",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "pnpm run db:generate && rm -rf dist && tsc",
    "lint": "biome lint",
    "lint:fix": "biome lint . --write",
    "format": "biome format . --write",
    "prepack": "npm run build",
    "watch": "npm run build -- --watch",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "jobs:dev": "trigger dev",
    "jobs:deploy": "trigger deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jozsefsallai/aronabot.git"
  },
  "author": "@jozsefsallai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jozsefsallai/aronabot/issues"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "homepage": "https://github.com/jozsefsallai/aronabot#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@aws-sdk/lib-storage": "^3.529.1",
    "@discordjs/rest": "^2.0.1",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@resvg/resvg-js": "^2.6.2",
    "@trigger.dev/sdk": "4.0.7",
    "axios": "^1.5.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "prisma": "^7.1.0",
    "pug": "^3.0.2",
    "react": "^18.3.1",
    "satori": "github:jozsefsallai/satori#sallai/webp-and-transform",
    "seedrandom": "^3.0.5",
    "sharp": "^0.33.5",
    "trigger.dev": "4.0.7",
    "tsx": "^4.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@trigger.dev/build": "4.0.7",
    "@types/node": "^20.6.3",
    "@types/react": "^18.3.5",
    "@types/seedrandom": "^3.0.8",
    "cheerio": "1.0.0-rc.12",
    "typescript": "^5.2.2"
  },
