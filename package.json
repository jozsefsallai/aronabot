{
  "name": "aronabot",
  "version": "1.0.0",
  "description": "Blue Archive themed Discord bot.",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "next lint",
    "lint:fix": "biome lint . --write",
    "format": "biome format . --write",
    "prepack": "npm run build",
    "watch": "npm run build -- --watch",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "scripts:seedStudents": "tsx src/scripts/seedStudents.ts",
    "scripts:seedMissions": "tsx src/scripts/seedMissions.ts",
    "scripts:seedGifts": "tsx src/scripts/seedGifts.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jozsefsallai/aronabot.git"
  },
  "author": "@jozsefsallai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jozsefsallai/aronabot/issues"
  },
  "homepage": "https://github.com/jozsefsallai/aronabot#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@aws-sdk/lib-storage": "^3.529.1",
    "@discordjs/rest": "^2.0.1",
    "@resvg/resvg-js": "^2.6.2",
    "axios": "^1.5.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.30.10",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "pug": "^3.0.2",
    "react": "^18.3.1",
    "satori": "github:jozsefsallai/satori#sallai/webp-and-transform",
    "seedrandom": "^3.0.5",
    "sharp": "^0.33.5",
    "tsx": "^4.7.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@prisma/client": "^6.5.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "@types/node": "^20.6.3",
    "@types/react": "^18.3.5",
    "@types/seedrandom": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "cheerio": "1.0.0-rc.12",
    "drizzle-kit": "^0.21.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "prisma": "^6.5.0",
    "typescript": "^5.2.2"
  }
